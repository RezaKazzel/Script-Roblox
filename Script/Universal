Rey:CreateToggle(Misc, "Full Bright", "-", function(state)
	state = not state
	local Lighting = game:GetService("Lighting")
	
	local function brightFunc()
		Lighting.Brightness = 2
		Lighting.ClockTime = 14
		Lighting.FogEnd = 100000
		Lighting.GlobalShadows = false
		Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
	end
	
	if not _G.brightLoop and state then
		_G.brightLoop = RunService.RenderStepped:Connect(brightFunc)
	elseif _G.brightLoop and not state then
		_G.brightLoop:Disconnect()
		_G.brightLoop = nil
	end
end)


Rey:CreateToggle(Misc, "Noclip", "-", function(state)
_G.Noclip = state
	if _G.Noclip then
		nc = RunService.Stepped:Connect(function()
			pcall(function()
				for _, child in pairs(Player.Character:GetDescendants()) do
					if child:IsA("BasePart") and child.CanCollide == true then
						child.CanCollide = false
					end
				end
			end)
		end)
	elseif not _G.Noclip then
		if nc then
			nc:Disconnect()
		end
	end
end)

Rey:CreateToggle(Misc, "Infinite Jump", "[may not work in some games]", function(state)
	if not _G.InfJump and state then
		if _G.InfJump then _G.InfJump:Disconnect() end
		_G.InfJump = game:GetService("UserInputService").JumpRequest:connect(function()
			if not infJumpDebounce then
				infJumpDebounce = true
				game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
				wait()
				infJumpDebounce = false
			end
		end)
	elseif _G.InfJump and not state then
		_G.InfJump:Disconnect()
		_G.InfJump = nil
	end
end)

local function getFolders()
	local parent = game.Workspace
	local folders = {}
	for _, descendant in ipairs(parent:GetDescendants()) do
		if descendant:IsA("Folder") then
			table.insert(folders, descendant)
		end
	end
	return folders
end

local function getPlayerList()
	local playerNames = {}
	for _, player in ipairs(game.Players:GetPlayers()) do
		table.insert(playerNames, player.Name)
	end
	return playerNames
end

-- Dropdown untuk teleport
Rey:CreateDropdown(Misc, "Teleport to Player", getPlayerList, function(selectedPlayer)
	local localPlayer = game.Players.LocalPlayer
	local targetPlayer = game.Players:FindFirstChild(selectedPlayer)

	if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
		local targetPosition = targetPlayer.Character.HumanoidRootPart.Position
		if localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") then
			localPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition)
		end
	end
end, true)

local NpcFolder = game.Workspace
local function getNPCList()
	local npc = {}
	for _, v in ipairs(NpcFolder:GetDescendants()) do
		if v.Name:lower() ~= "keyframe" then
			if v:FindFirstChild("CFrame") or v:FindFirstChild("HumanoidRootPart") then
				if checksTable(npc, v) then
					table.insert(npc, v)
				end
			end
		end
	end
	return npc
end

Rey:CreateDropdown(Misc, "Teleport to NPC", getNPCList, function(selectedNpc)
	local localPlayer = game.Players.LocalPlayer
	local target = selectedNpc
	if target:FindFirstChild("CFrame") then
		targetPosition = target.CFrame
	elseif target:FindFirstChild("HumanoidRootPart") then
		targetPosition = target.HumanoidRootPart.Position
	end
	if localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") then
		localPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition)
	end
end, true)

