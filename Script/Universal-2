-- Cleanup Events
local removeConn
removeConn = Players.PlayerRemoving:Connect(function(leavingPlayer)
    if leavingPlayer == LocalPlayer then
        removeConn:Disconnect()
        Cleanup.All()
    end
end)

UI.AncestryChanged:Connect(function(_, parent)
    if not parent then
        Cleanup.All()
    end
end)